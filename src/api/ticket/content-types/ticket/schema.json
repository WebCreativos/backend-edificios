{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "tickets"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "Low",
        "Medium",
        "High"
      ]
    },
    "area": {
      "type": "string"
    },
    "apartment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::apartament.apartament"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Open",
        "Closed"
      ],
      "default": "Open"
    }
  }
}
